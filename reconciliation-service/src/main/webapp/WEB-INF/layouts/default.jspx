<html
	xmlns:jsp="http://java.sun.com/JSP/Page" 
	xmlns:c="http://java.sun.com/jsp/jstl/core" 
	xmlns:tiles="http://tiles.apache.org/tags-tiles" 
	xmlns:spring="http://www.springframework.org/tags" 
	xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" >

	<jsp:output omit-xml-declaration="yes"/>
	<jsp:output doctype-root-element="HTML" doctype-system="about:legacy-compat" />
	<jsp:directive.page contentType="text/html;charset=UTF-8" />
	<jsp:directive.page pageEncoding="UTF-8" />

	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="edge" />

		<!-- Following line replaced by Kew CSS if configured -->
		<link href="/css/default.css" rel="stylesheet" />

		<title><tiles:insertAttribute name="title"/></title>

		<style><![CDATA[
			.supplied { background-color: lightgreen; }
			.linked { background-color: lightblue; }
		]]></style>

		<!-- jQuery -->
		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js">Â </script>
	</head>

	<body>
		<!-- Following element replaced by Kew header if configured -->
		<header id='kew-header' />

		<spring:url var="index" value="/" />
		<ul id="breadcrumbs">
			<li>Where am I?
				<ul>
					<li><a href="http://www.kew.org/" class="breadcrumb">Home</a>
						<ul>
							<li><a href="http://www.kew.org/data/" class="breadcrumb">Kew Databases</a>
								<ul>
									<li><a href="${index}" class="breadcrumb">Reconciliation Service (preview)</a></li>
								</ul>
							</li>
						</ul>
					</li>
				</ul>
			</li>
		</ul>

		<div class="scidb four-col float-left" id="wrapper">
			<div id="main">
				<tiles:insertAttribute name="body"/>
			</div>

			<tiles:insertAttribute name="footer" ignore="true"/>
		</div>

		<tiles:insertAttribute name="header" ignore="true"  />

		<!-- Following element replaced by Kew footer if configured -->
		<footer id='kew-footer' />
	</body>
</html>
